import random
import math
def accept_prob(S_old, S_new, T):
    if S_new > S_old:
        return 1.0
    else:
        diff = S_old - S_new
        return math.exp(-diff / T)
def accept(S_old, S_new, T):
    if random.random() < accept_prob(S_old, S_new, T):
        print(True)
    else:
        print(False)
